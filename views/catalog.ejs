              <button type="button" class="btn btn-panel-fix" <% if (user) { %>onclick="window.open('/account/',
                '_self');"<% } else { %>onclick="window.open('/authentication/', '_self');"<% } %>>
                    <% if (user) { %><i class="fa-solid fa-user-check"></i>
                      <% } else { %><i class="fas fa-user"></i>
                        <% } %>
              </button>
              <button <% if (user) { %>style="margin-left: 4px;"<% } %> type="button" class="btn btn-panel-fix"
                  onclick="window.open('/cart/', '_self');">
                  <i class="fas fa-shopping-cart"></i>
                  <span id="cart_length_mobile"
                    style="position: absolute; top: -8px; right: -8px; min-width: 18px; background: #dc3545; border-radius: 50px; color: #fff; font-size: 12px; padding-left: 5px; padding-right: 5px;">
                    <% if (cart) { %>
                      <%= cart %>
                        <% } %>
                  </span>
              </button>
              <button type="button" class="btn btn-panel-fix" data-bs-toggle="offcanvas"
